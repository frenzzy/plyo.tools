{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "default-case": 0,
    "generator-star-spacing": 0,
    "import/no-extraneous-dependencies": 0,
    "max-len": [2, 120],
    "no-param-reassign": [2, {"props": false}],
    "prefer-template": 0,
    "new-parens": 0,
    "no-mixed-operators": 0,
    "require-yield": 0,
    "no-console": 0
  }
}
